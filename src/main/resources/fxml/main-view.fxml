                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </Label>
                                          <HBox maxWidth="1.7976931348623157E308" spacing="5.0">
                                             <children>
                                                <DatePicker maxWidth="1.7976931348623157E308" promptText="Date" HBox.hgrow="SOMETIMES" />
                                                <TextField minWidth="-Infinity" prefWidth="40.0" promptText="H" HBox.hgrow="SOMETIMES" />
                                                <TextField minWidth="-Infinity" prefWidth="40.0" promptText="M" HBox.hgrow="SOMETIMES" />
                                                <TextField minWidth="-Infinity" prefWidth="40.0" promptText="S" HBox.hgrow="SOMETIMES" />
                                                <ChoiceBox minWidth="-Infinity" prefWidth="60.0" HBox.hgrow="SOMETIMES" />
                                             </children>
                                          </HBox>
                                          <Label text="Location">
                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </Label>
                                          <ChoiceBox maxWidth="1.7976931348623157E308" prefWidth="150.0" />
