                                        <TableColumn prefWidth="75.0" text="Vehicle ID" />
                                        <TableColumn prefWidth="75.0" text="Model" />
                                          <TableColumn prefWidth="75.0" text="Capacity" />
                                          <TableColumn prefWidth="75.0" text="Location" />
                                          <TableColumn prefWidth="75.0" text="Status" />
                                      </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                    <VBox fx:id="formVehicle" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
                                       <children>
                                          <HBox alignment="CENTER_LEFT" VBox.vgrow="NEVER">
                                             <VBox.margin>
                                                <Insets />
                                             </VBox.margin>
                                             <children>
                                                <Label text="Vehicle">
                                                   <font>
                                                      <Font size="16.0" />
                                                   </font>
                                                </Label>
                                                <Pane prefHeight="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
                                                <Button mnemonicParsing="false" onAction="#onNewVehicle" prefWidth="80.0" text="New" />
                                             </children>
                                          </HBox>
                                          <Label text="Vehicle ID">
                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </Label>
                                          <TextField promptText="ID" />
