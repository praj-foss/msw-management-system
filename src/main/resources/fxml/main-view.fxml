                                       <children>
                                          <HBox alignment="CENTER_LEFT" VBox.vgrow="NEVER">
                                             <VBox.margin>
                                                <Insets />
                                             </VBox.margin>
                                             <children>
                                                <Label text="Collection">
                                                   <font>
                                                      <Font size="16.0" />
                                                   </font>
                                                </Label>
                                                <Pane prefHeight="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
